pipeline {
    agent { label 'work_station'}

    stages{
        stage ('stage one'){
            steps{
                sh: 'git clone https://github.com/ChaitanyaChandra/ansible-lab.git'
                sh: 'cd ansible-lab'
                }
            }

        stage ('stage two'){
            steps{
                sh: 'ansible -m ping all -i roboshop.ini'
            }
        }
    }
}