pipeline {
    agent any

    stages{
        stage ('stage one'){
            steps{
                step{
                    echo "hello from stage one, step one"
                }
                step{
                    echo "hello from stage one, step two"
                }
            }
        }

        stage ('stage one'){
            steps{
                step{
                    echo "hello from stage two, step one"
                }
                step{
                    echo "hello from stage two, step two"
                }
            }
        }
    }

}